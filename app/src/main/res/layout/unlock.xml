<HorizontalScrollView xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent" android:layout_height="wrap_content" >

	<ScrollView android:id="@+id/unlock" 
		android:layout_width="wrap_content" 
		android:layout_height="match_parent" >

		<RelativeLayout android:id="@+id/content_container" 
			android:layout_width="wrap_content" 
			android:layout_height="wrap_content" >

			<View   android:id="@+id/_bg__unlock"
				android:background="@drawable/_bg__unlock_shape"
				android:layout_width="412dp"
				android:layout_height="892dp" />


						<RelativeLayout android:id="@+id/head_bar"
				android:layout_marginTop="40dp"
				android:layout_marginStart="0dp"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				>

				<ImageView android:id="@+id/vector_1"
					android:src="@drawable/vector_1"
					android:contentDescription="vector_1"
					 android:layout_marginStart="0dp"
					android:layout_marginTop="0dp"
					android:layout_width="412dp"
					android:layout_height="80dp" />

				<ImageView android:id="@+id/add"
					android:src="@drawable/add"
					android:contentDescription="add"
					 android:layout_marginStart="250dp"
					android:layout_marginTop="15dp"
					android:layout_width="50dp"
					android:layout_height="50dp" />

				<ImageView android:id="@+id/tune"
					android:src="@drawable/tune"
					android:contentDescription="tune"
					 android:layout_marginStart="330dp"
					android:layout_marginTop="15dp"
					android:layout_width="50dp"
					android:layout_height="50dp" />

			</RelativeLayout>


			<RelativeLayout android:id="@+id/logo"
				android:layout_marginTop="830dp"
				android:layout_centerHorizontal="true"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				>

				<View   android:id="@+id/_bg__logo_ek1"
					android:background="@drawable/_bg__logo_ek1_shape"
					 android:layout_marginStart="0dp"
					android:layout_marginTop="0dp"
					android:layout_width="match_parent"
					android:layout_height="match_parent" />

				<ImageView android:id="@+id/image_3"
					android:src="@drawable/image_3"
					android:contentDescription="image_3"
					 android:layout_marginStart="0dp"
					android:layout_marginTop="0dp"
					android:layout_width="match_parent"
					android:layout_height="match_parent" />

			</RelativeLayout>


			<RelativeLayout android:id="@+id/fotter"
				android:layout_marginTop="700dp"
				android:layout_marginStart="0dp"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				>

				<View   android:id="@+id/_bg__fotter_ek1"
					android:background="@drawable/_bg__fotter_ek1_shape"
					 android:layout_marginStart="0dp"
					android:layout_marginTop="0dp"
					android:layout_width="412dp"
					android:layout_height="90dp" />

				<View   android:id="@+id/rectangle_51"
					android:background="@drawable/rectangle_51_shape"
					 android:layout_marginStart="0dp"
					android:layout_marginTop="0dp"
					android:layout_width="412dp"
					android:layout_height="90dp" />

				<ImageView android:id="@+id/vector"
					android:src="@drawable/vector"
					android:contentDescription="vector"
					 android:layout_marginStart="64dp"
					android:layout_marginTop="20dp"
					android:layout_width="60dp"
					android:layout_height="60dp" />

				<ImageView android:id="@+id/combined_shape"
					android:src="@drawable/combined_shape"
					android:contentDescription="combined_shape"
					 android:layout_marginStart="294dp"
					android:layout_marginTop="20dp"
					android:layout_width="60dp"
					android:layout_height="60dp" />

			</RelativeLayout>


			<RelativeLayout android:id="@+id/lock"
				android:layout_marginTop="320dp"
				android:layout_centerHorizontal="true"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				>

				<ImageView android:id="@+id/btn_unlock"
					android:src="@drawable/vector_ek1"
					android:contentDescription="vector_ek1"
					 android:layout_marginStart="0dp"
					android:layout_marginTop="0dp"
					android:layout_width="290dp"
					android:layout_height="340dp" />

			</RelativeLayout>

			<TextView android:id="@+id/nome_fechadura"
				android:layout_centerHorizontal="true"
				android:gravity="center"
				android:textSize="36sp"
				android:textColor="@color/time_color"
				android:layout_marginTop="180dp"
				android:layout_width="244dp"
				android:layout_height="50dp"
				android:text="@string/nome_fechadura_string"
				 />

			<TextView android:id="@+id/clique_no_cadeado_para_abrir"
				android:layout_centerHorizontal="true"
				android:gravity="center"
				android:textSize="22sp"
				android:textColor="@color/click_unlock"
				android:layout_marginTop="260dp"
				android:layout_width="match_parent"
				android:layout_height="match_parent"
				android:text="@string/clique_no_cadeado_para_abrir_string"
				 />

		</RelativeLayout>

	</ScrollView>
</HorizontalScrollView>